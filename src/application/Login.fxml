<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane blendMode="DIFFERENCE" prefHeight="600.0" prefWidth="600.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Button layoutX="202.0" layoutY="432.0" mnemonicParsing="false" onAction="#Login" text="Login">
         <effect>
            <Lighting bumpInput="$null">
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Button>
      <PasswordField fx:id="txtpassword" layoutX="81.0" layoutY="283.0" promptText="Password">
         <effect>
            <Reflection fraction="0.94" topOpacity="0.44" />
         </effect></PasswordField>
      <TextField fx:id="txtusername" layoutX="81.0" layoutY="172.0" promptText="Username">
         <effect>
            <Reflection />
         </effect></TextField>
      <Sphere layoutX="203.0" layoutY="92.0" radius="0.77" />
      <Label fx:id="poslogin" layoutX="160.0" layoutY="58.0" text="POS Login">
         <font>
            <Font size="47.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </Label>
   </children>
   <effect>
      <InnerShadow />
   </effect>
</AnchorPane>
